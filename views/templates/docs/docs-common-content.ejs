<!-- TOP Navigation arrows -->
<div class="content-navigation top">
    <a class="left" ng-show="prevUrl()" ng-href="{{prevUrl()}}">
        <!--<span class="glyphicon glyphicon-chevron-left"></span>-->
        <span>Previous</span>
    </a>
    <a class="right" ng-show="nextUrl()" ng-href="{{nextUrl()}}">
        <span>Next</span>
        <!--<span class="glyphicon glyphicon-chevron-right"></span>-->
    </a>
</div>

<!-- BOTTOM Navigation arrows-->
<div class="content-navigation bottom">
    <a class="left" ng-show="prevUrl()" ng-href="{{prevUrl()}}">
        <!--<span class="glyphicon glyphicon-chevron-left"></span>-->
        <span>Previous</span>
    </a>
    <a class="right" ng-show="nextUrl()" ng-href="{{nextUrl()}}">
        <!--<span class="glyphicon glyphicon-chevron-right"></span>-->
        <span>Next</span>
    </a>
</div>

<!-- Toggle side panel button -->
<div id="side-panel-toggle">
    <button type="button" class="btn" ng-click="toggleWrapper()">
        <span class="glyphicon glyphicon-menu-hamburger"></span>
    </button>
</div>